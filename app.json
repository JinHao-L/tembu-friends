{
    "expo": {
        "name": "TembuFriends",
        "slug": "TembuFriends",
        "scheme": "tembufriends",
        "notification": {
            "icon": "./App/assets/images/notification-icon.png",
            "androidMode": "default",
            "iosDisplayInForeground": true
        },
        "platforms": ["ios", "android", "web"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./App/assets/images/icon.png",
        "splash": {
            "image": "./App/assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "bundleIdentifier": "com.nus.tembufriends",
            "buildNumber": "1.0.0",
            "supportsTablet": true
        },
        "android": {
            "package": "com.nus.tembufriends",
            "versionCode": 1,
            "googleServicesFile": "",
            "permissions": []
        },
        "web": {
            "favicon": "./assets/images/favicon.png"
        },
        "description": "A social app for Tembusians (An Apollo-11 Project)"
    }
}
