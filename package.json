{
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "test": "jest --watch --coverage=false --changedSince=origin/master",
        "testDebug": "jest -o --watch --coverage=false",
        "testFinal": "jest",
        "updateSnapshots": "jest -u --coverage=false"
    },
    "jest": {
        "projects": [
            {
                "preset": "jest-expo/ios"
            },
            {
                "preset": "jest-expo/android"
            }
        ],
        "testMatch": [
            ".**/__tests__/**/*.js?(x)"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
        ]
    },
    "dependencies": {
        "@react-native-community/masked-view": "0.1.10",
        "@react-navigation/bottom-tabs": "^5.6.1",
        "@react-navigation/native": "^5.6.1",
        "@react-navigation/stack": "^5.6.2",
        "@testing-library/react-native": "^7.1.0",
        "expo": "^40.0.0",
        "expo-asset": "~8.2.1",
        "expo-barcode-scanner": "~9.1.0",
        "expo-font": "~8.4.0",
        "expo-image-picker": "~9.2.0",
        "expo-linear-gradient": "~8.4.0",
        "expo-notifications": "~0.8.2",
        "expo-sharing": "~8.5.0",
        "firebase": "7.9.0",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.0.tar.gz",
        "react-native-custom-qr-codes-expo": "^2.2.0",
        "react-native-dotenv": "^2.4.3",
        "react-native-elements": "^3.0.0",
        "react-native-gesture-handler": "~1.8.0",
        "react-native-image-zoom-viewer": "^3.0.1",
        "react-native-picker-select": "^8.0.4",
        "react-native-progress": "^4.1.2",
        "react-native-reanimated": "~1.13.0",
        "react-native-safe-area-context": "3.1.9",
        "react-native-screens": "~2.15.0",
        "react-native-svg": "12.1.0",
        "react-native-tab-view": "^2.15.0",
        "react-native-vector-icons": "^7.0.0",
        "react-native-view-more-text": "^2.1.0",
        "react-native-view-shot": "3.1.2",
        "react-native-web": "~0.13.12",
        "react-native-webview": "11.6.4",
        "react-redux": "^7.2.2",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "~7.9.0",
        "babel-preset-expo": "8.3.0",
        "husky": "^4.2.5",
        "jest-expo": "^40.0.0",
        "prettier": "^2.0.5",
        "pretty-quick": "^3.1.0",
        "react-test-renderer": "^17.0.1"
    },
    "resolutions": {
        "logkitty": "^0.7.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "prettier": {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100,
        "tabWidth": 4
    }
}
